  "devDependencies": {
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "express": "^5.1.0",
    "node-firebird": "^1.1.9"
  },
  "build": {
    "appId": "com.cesaria.cesariaonline",
    "productName": "Ces√°ria Online",
    "files": [
